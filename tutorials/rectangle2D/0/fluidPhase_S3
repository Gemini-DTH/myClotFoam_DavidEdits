FoamFile
{
    format      ascii;
    class       volScalarField;
    object      fluidPhase_S3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "$FOAM_CASE/constant/inputParameters"

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    inletVessel
    {
        type         uniformFixedValue;
        uniformValue
        {
            type             scale;
            scale            linearRamp;
            start            $t0_chem;
            duration         $tf_chem;
            value            $S3_IC;
        }
    }
    
    outletVessel
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }
    
    injuryWalls
    {
        type            zeroGradient;
    }
    

    frontAndBack
    {
        type            empty;
    }
}

